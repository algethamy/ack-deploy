{
    "name": "algethamy/laravel-ack-deploy",
    "description": "Laravel package for easy deployment to Alibaba Cloud Container Service for Kubernetes (ACK)",
    "keywords": ["laravel", "alibaba-cloud", "ack", "kubernetes", "deployment", "docker"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Algethamy",
            "email": "algethamy@example.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "laravel/framework": "^10.0|^11.0",
        "symfony/console": "^6.0|^7.0",
        "symfony/process": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Algethamy\\LaravelAckDeploy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Algethamy\\LaravelAckDeploy\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Algethamy\\LaravelAckDeploy\\LaravelAckDeployServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}